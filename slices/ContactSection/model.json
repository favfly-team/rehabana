            "thumbnails": []
          }
        },
        "items": {
          "type": "Group",
          "config": {
            "label": "Items",
            "repeat": true,
            "fields": {
              "title": {
                "type": "StructuredText",
                "config": {
                  "label": "Title",
                  "placeholder": "e.g. Phone, Email, Address",
                  "allowTargetBlank": false,
                  "single": "heading3,strong"
                }
              },
              "details": {
                "type": "StructuredText",
                "config": {
                  "label": "Details",
                  "placeholder": "",
                  "allowTargetBlank": true,
                  "single": "paragraph,strong,hyperlink"
                }
              },
              "link": {
                "type": "Link",
                "config": {
                  "label": "Link",
                  "placeholder": "tel: or mailto:",
                  "select": null,
                  "allowTargetBlank": true,
                  "allowText": true
                }
              }
            }
          }
        }
      },
      "items": {}
    }
